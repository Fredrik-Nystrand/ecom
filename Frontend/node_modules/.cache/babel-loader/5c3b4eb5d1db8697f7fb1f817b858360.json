{"ast":null,"code":"import CartItem from \"./CartItemComp\";\nimport { computed, ref } from \"vue\";\nimport { useStore } from \"vuex\";\nimport { useRouter, useRoute } from \"vue-router\";\nexport default {\n  name: \"Checkout\",\n  components: {\n    CartItem\n  },\n\n  setup() {\n    const router = useRouter();\n    const route = useRoute();\n    const store = useStore();\n    const cart = ref(computed(() => store.getters.cart));\n    const totalPrice = ref(computed(() => store.getters.cart.reduce((a, b) => a + b.price * b.amount, 0)));\n    const noVat = computed(() => totalPrice.value * 0.8);\n\n    const checkout = () => {\n      router.push({\n        name: \"checkout\"\n      });\n    };\n\n    return {\n      cart,\n      totalPrice,\n      noVat,\n      checkout\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAOA,OAAOA,QAAP,MAAqB,gBAArB;AACA,SAASC,QAAT,EAAmBC,GAAnB,QAA8B,KAA9B;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,SAASC,SAAT,EAAoBC,QAApB,QAAoC,YAApC;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;AAEbC,YAAU,EAAE;AACVP;AADU,GAFC;;AAKbQ,OAAK,GAAG;AACN,UAAMC,MAAK,GAAIL,SAAS,EAAxB;AACA,UAAMM,KAAI,GAAIL,QAAQ,EAAtB;AACA,UAAMM,KAAI,GAAIR,QAAQ,EAAtB;AACA,UAAMS,IAAG,GAAIV,GAAG,CAACD,QAAQ,CAAC,MAAMU,KAAK,CAACE,OAAN,CAAcD,IAArB,CAAT,CAAhB;AACA,UAAME,UAAS,GAAIZ,GAAG,CACpBD,QAAQ,CAAC,MACPU,KAAK,CAACE,OAAN,CAAcD,IAAd,CAAmBG,MAAnB,CAA0B,CAACC,CAAD,EAAIC,CAAJ,KAAUD,IAAIC,CAAC,CAACC,KAAF,GAAUD,CAAC,CAACE,MAApD,EAA4D,CAA5D,CADM,CADY,CAAtB;AAMA,UAAMC,KAAI,GAAInB,QAAQ,CAAC,MAAMa,UAAU,CAACO,KAAX,GAAmB,GAA1B,CAAtB;;AAEA,UAAMC,QAAO,GAAI,MAAM;AACrBb,YAAM,CAACc,IAAP,CAAY;AAAEjB,YAAI,EAAE;AAAR,OAAZ;AACD,KAFD;;AAIA,WAAO;AACLM,UADK;AAELE,gBAFK;AAGLM,WAHK;AAILE;AAJK,KAAP;AAMD;;AA5BY,CAAf","names":["CartItem","computed","ref","useStore","useRouter","useRoute","name","components","setup","router","route","store","cart","getters","totalPrice","reduce","a","b","price","amount","noVat","value","checkout","push"],"sourceRoot":"","sources":["C:\\Users\\nystr\\OneDrive\\Dokument\\skola\\Frontendutv\\Javascript