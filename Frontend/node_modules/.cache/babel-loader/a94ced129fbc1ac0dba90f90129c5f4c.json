{"ast":null,"code":"import CartItem from \"./CartItemComp\";\nimport { computed, ref, watch } from \"vue\";\nimport { useStore } from \"vuex\";\nimport { useRouter, useRoute } from \"vue-router\";\nexport default {\n  name: \"CartList\",\n  components: {\n    CartItem\n  },\n\n  setup(props, {\n    emit\n  }) {\n    const router = useRouter();\n    const route = useRoute();\n    const store = useStore();\n    const cart = ref(computed(() => store.getters.cart));\n    const totalPrice = ref(computed(() => store.getters.cart.reduce((a, b) => a + b.price * b.amount, 0)));\n    const noVat = computed(() => totalPrice.value * 0.8);\n\n    const checkout = () => {\n      router.push({\n        name: \"checkout\"\n      });\n      emit(\"closeCart\");\n    };\n\n    return {\n      cart,\n      totalPrice,\n      noVat,\n      checkout\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiBA,OAAOA,QAAP,MAAqB,gBAArB;AACA,SAASC,QAAT,EAAmBC,GAAnB,EAAwBC,KAAxB,QAAqC,KAArC;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,SAASC,SAAT,EAAoBC,QAApB,QAAoC,YAApC;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;AAEbC,YAAU,EAAE;AACVR;AADU,GAFC;;AAKbS,OAAK,CAACC,KAAD,EAAQ;AAAEC;AAAF,GAAR,EAAkB;AACrB,UAAMC,MAAK,GAAIP,SAAS,EAAxB;AACA,UAAMQ,KAAI,GAAIP,QAAQ,EAAtB;AACA,UAAMQ,KAAI,GAAIV,QAAQ,EAAtB;AACA,UAAMW,IAAG,GAAIb,GAAG,CAACD,QAAQ,CAAC,MAAMa,KAAK,CAACE,OAAN,CAAcD,IAArB,CAAT,CAAhB;AACA,UAAME,UAAS,GAAIf,GAAG,CACpBD,QAAQ,CAAC,MACPa,KAAK,CAACE,OAAN,CAAcD,IAAd,CAAmBG,MAAnB,CAA0B,CAACC,CAAD,EAAIC,CAAJ,KAAUD,IAAIC,CAAC,CAACC,KAAF,GAAUD,CAAC,CAACE,MAApD,EAA4D,CAA5D,CADM,CADY,CAAtB;AAMA,UAAMC,KAAI,GAAItB,QAAQ,CAAC,MAAMgB,UAAU,CAACO,KAAX,GAAmB,GAA1B,CAAtB;;AAEA,UAAMC,QAAO,GAAI,MAAM;AACrBb,YAAM,CAACc,IAAP,CAAY;AAAEnB,YAAI,EAAE;AAAR,OAAZ;AACAI,UAAI,CAAC,WAAD,CAAJ;AACD,KAHD;;AAKA,WAAO;AACLI,UADK;AAELE,gBAFK;AAGLM,WAHK;AAILE;AAJK,KAAP;AAMD;;AA7BY,CAAf","names":["CartItem","computed","ref","watch","useStore","useRouter","useRoute","name","components","setup","props","emit","router","route","store","cart","getters","totalPrice","reduce",