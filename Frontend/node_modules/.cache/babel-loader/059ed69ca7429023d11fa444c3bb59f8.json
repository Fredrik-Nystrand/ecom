{"ast":null,"code":"import { ref, watch, computed } from \"vue\";\nimport { useRouter } from \"vue-router\";\nimport { useStore } from \"vuex\";\nexport default {\n  name: \"LoginView\",\n\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n    const email = ref(\"\");\n    const password = ref(\"\");\n\n    const handleSubmit = async () => {\n      if (email.value.trim() === \"\" || password.value.trim() === \"\") {\n        return;\n      }\n\n      const user = {\n        email: email.value,\n        password: password.value\n      };\n      const newUser = await store.dispatch(\"login\", user);\n\n      if (newUser) {\n        router.push({\n          name: home\n        });\n      }\n    };\n\n    return {\n      email,\n      password,\n      handleSubmit\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAuCA,SAASA,GAAT,EAAcC,KAAd,EAAqBC,QAArB,QAAqC,KAArC;AACA,SAASC,SAAT,QAA0B,YAA1B;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,eAAe;AACbC,MAAI,EAAE,WADO;;AAEbC,OAAK,GAAG;AACN,UAAMC,KAAI,GAAIH,QAAQ,EAAtB;AACA,UAAMI,MAAK,GAAIL,SAAS,EAAxB;AACA,UAAMM,KAAI,GAAIT,GAAG,CAAC,EAAD,CAAjB;AACA,UAAMU,QAAO,GAAIV,GAAG,CAAC,EAAD,CAApB;;AAEA,UAAMW,YAAW,GAAI,YAAY;AAC/B,UAAIF,KAAK,CAACG,KAAN,CAAYC,IAAZ,OAAuB,EAAvB,IAA6BH,QAAQ,CAACE,KAAT,CAAeC,IAAf,OAA0B,EAA3D,EAA+D;AAC7D;AACF;;AACA,YAAMC,IAAG,GAAI;AACXL,aAAK,EAAEA,KAAK,CAACG,KADF;AAEXF,gBAAQ,EAAEA,QAAQ,CAACE;AAFR,OAAb;AAKA,YAAMG,OAAM,GAAI,MAAMR,KAAK,CAACS,QAAN,CAAe,OAAf,EAAwBF,IAAxB,CAAtB;;AAEA,UAAIC,OAAJ,EAAa;AACXP,cAAM,CAACS,IAAP,CAAY;AAAEZ,cAAI,EAAEa;AAAR,SAAZ;AACF;AACD,KAdD;;AAgBA,WAAO;AACLT,WADK;AAELC,cAFK;AAGLC;AAHK,KAAP;AAKD;;AA7BY,CAAf","names":["ref","watch","computed","useRouter","useStore","name","setup","store","router","email","password","handleSubmit","value","trim","user","newUser","dispatch","push","home"],"sourceRoot":"","sources":["C:\\Users\\nystr\\OneDrive\\Dokument\\skola\\Frontendutv\\Javascript2\\Frontend\\app\\src\\views\\LoginView.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-wrapper container\">\r\n    <div class=\"login-box card\">\r\n      <h1>Login</h1>\r\n      <form class=\"login__form form\" @submit.prevent=\"handleSubmit\">\r\n        <div class=\"form-group\">\r\n          <input\r\n            class=\"input\"\r\n            type=\"email\"\r\n            placeholder=\"Email\"\r\n            v-model=\"email\"\r\n          />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <input\r\n            class=\"input\"\r\n            type=\"password\"\r\n            placeholder=\"Password\"\r\n            v-model=\"password\"\r\n          />\r\n        </div>\r\n        <div class=\"form-row\">\r\n          <div class=\"form-group\">\r\n            <input class=\"checkbox\" type=\"checkbox\" id=\"remember\" />\r\n            <label for=\"remember\">Remember me</label>\r\n          </div>\r\n          <a href=\"#\">Forgot password?</a>\r\n        </div>\r\n        <button class=\"btn btn-primary\">Sign In</button>\r\n      </form>\r\n      <div class=\"sign-up-link\">\r\n        Not a member yet?\r\n        <router-link to=\"/register\">Register Now!</router-link>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, watch, computed } from \"vue\";\r\nimport { useRouter } from \"vue-router\";\r\nimport { useStore } from \"vuex\";\r\nexport default {\r\n  name: \"LoginView\",\r\n  setup() {\r\n    const store = useStore();\r\n    const router = useRouter();\r\n    const email = ref(\"\");\r\n    const password = ref(\"\");\r\n\r\n    const handleSubmit = async () => {\r\n      if (email.value.trim() === \"\" || password.value.trim() === \"\") {\r\n        return;\r\n      }\r\n      const user = {\r\n        email: email.value,\r\n        password: password.value,\r\n      };\r\n\r\n      const newUser = await store.dispatch(\"login\", user);\r\n\r\n      if (newUser) {\r\n        router.push({ name: home });\r\n      }\r\n    };\r\n\r\n    return {\r\n      email,\r\n      password,\