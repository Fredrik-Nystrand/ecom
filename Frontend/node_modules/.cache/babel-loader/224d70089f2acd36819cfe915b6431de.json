{"ast":null,"code":"import CartItem from \"./CartItemComp\";\nimport { computed, ref, watch } from \"vue\";\nimport { useStore } from \"vuex\";\nimport { useRouter, useRoute } from \"vue-router\";\nexport default {\n  name: \"CartList\",\n  components: {\n    CartItem\n  },\n\n  setup() {\n    const router = useRouter();\n    const route = useRoute();\n    const store = useStore();\n    const cart = ref(computed(() => store.getters.cart));\n    const totalPrice = ref(computed(() => cart.getters.totalPrice()));\n    const noVat = computed(() => totalPrice.value * 0.8);\n\n    const checkout = () => {\n      router.push({\n        name: \"checkout\"\n      });\n    };\n\n    return {\n      cart,\n      totalPrice,\n      noVat,\n      checkout\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiBA,OAAOA,QAAP,MAAqB,gBAArB;AACA,SAASC,QAAT,EAAmBC,GAAnB,EAAwBC,KAAxB,QAAqC,KAArC;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,SAASC,SAAT,EAAoBC,QAApB,QAAoC,YAApC;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;AAEbC,YAAU,EAAE;AACVR;AADU,GAFC;;AAKbS,OAAK,GAAG;AACN,UAAMC,MAAK,GAAIL,SAAS,EAAxB;AACA,UAAMM,KAAI,GAAIL,QAAQ,EAAtB;AACA,UAAMM,KAAI,GAAIR,QAAQ,EAAtB;AACA,UAAMS,IAAG,GAAIX,GAAG,CAACD,QAAQ,CAAC,MAAMW,KAAK,CAACE,OAAN,CAAcD,IAArB,CAAT,CAAhB;AACA,UAAME,UAAS,GAAIb,GAAG,CAACD,QAAQ,CAAC,MAAMY,IAAI,CAACC,OAAL,CAAaC,UAAb,EAAP,CAAT,CAAtB;AAEA,UAAMC,KAAI,GAAIf,QAAQ,CAAC,MAAMc,UAAU,CAACE,KAAX,GAAmB,GAA1B,CAAtB;;AAEA,UAAMC,QAAO,GAAI,MAAM;AACrBR,YAAM,CAACS,IAAP,CAAY;AAAEZ,YAAI,EAAE;AAAR,OAAZ;AACD,KAFD;;AAIA,WAAO;AACLM,UADK;AAELE,gBAFK;AAGLC,WAHK;AAILE;AAJK,KAAP;AAMD;;AAxBY,CAAf","names":["CartItem","computed","ref","watch","useStore","useRouter","useRoute","name","components","setup","router","route","store","cart","getters","totalPrice","noVat","value","checkout","push"],"sourceRoot":"","sources":["C:\\Users\\nystr\\OneDrive\\Dokument\\skola\\Frontendutv\\Javascript2\\Frontend\\app\\src\\components\\cart\\CartListComp.vue"],"sourcesContent":["<template>\r\n  <div :class=\"{ underline: cart.length > 1, 'cart-list': true }\">\r\n    <div class=\"empty-cart\" v-if=\"cart.length < 1\">Your cart is empty</div>\r\n    <CartItem v-for=\"item in cart\" :key=\"item.id\" :item=\"item\" />\r\n    <div class=\"cart-list__footer\">\r\n      <div class=\"cart-list__price-wrapper\">\r\n        <p class=\"cart-list__total-price\">Total Price: {{ totalPrice }} SEK</p>\r\n        <p class=\"cart-list__vat-price\">VAT Excluded: {{ noVat }} SEK</p>\r\n      </div>\r\n      <button @click=\"checkout\" class=\"btn btn-primary\">\r\n        Checkout <i class=\"fa-solid fa-arrow-right\"></i>\r\n      </button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport CartItem from \"./CartItemComp\";\r\nimport { computed, ref, watch } from \"vue\";\r\nimport { useStore } from \"vuex\";\r\nimport { useRouter, useRoute } from \"vue-router\";\r\n\r\nexport default {\r\n  name: \"CartList\",\r\n  components: {\r\n    CartItem,\r\n  },\r\n  setup() {\r\n    const router = useRouter();\r\n    const route = useRoute();\r\n    const store = useStore();\r\n    const cart = ref(computed(() => store.getters.cart));\r\n    const totalPrice = ref(computed(() => cart.getters.totalPrice()));\r\n\r\n    const noVat = computed(() => totalPrice.value * 0.8);\r\n\r\n    const checkout = () => {\r\n      router.push({ name: \"checkout\" });\r\n    };\r\n\r\n    return {\r\n      cart,\r\n      totalPrice,\r\n      noVat,\r\n      checkout,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.cart-list {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 1rem;\r\n}\r\n.cart-list__footer {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.cart-list__total-price {\r\n  font-weight: bold;\r\n}\r\n\r\n.cart-list__vat-price {\r\n  font-size: small;\r\n  color: rgb(131, 131, 131);\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}